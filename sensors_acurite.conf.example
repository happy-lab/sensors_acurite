[logging]
# Levels from the Python logging module:
#    CRITICAL  50
#    ERROR     40
#    WARNING   30
#    INFO      20
#    DEBUG     10
#    NOTSET    0
level = 40

[collector]
interval = 60

[mqtt]
topic = home/sensor
host = localhost
port = 1883

[sensors]
#acurite/datetime = datetime
acurite/sensor_battery = sensor_battery
acurite/rssi = rssi
office/pressure = pressure
office/temperature = inside_temperature
garden/humidity = outside_humidity
garden/rain = rain
garden/rain_total = rain_total
garden/temperature = outside_temperature
garden/wind_direction = wind_direction
garden/wind_speed = wind_speed

[calculate]
dewpoint = outside

